    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "string-width": "^4.2.0",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
