{
  "name": "enigmatik",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm:electron:dev\"",
    "electron:dev": "tsc -p tsconfig.electron.json --watch & electron ./dist-electron/main.js",
    "build": "vite build && tsc -p tsconfig.electron.json",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "build:electron": "tsc -p tsconfig.electron.json",
    "electron:start": "electron ./dist-electron/main.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
